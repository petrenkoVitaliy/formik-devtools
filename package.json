{
    "name": "formik-devtools",
    "version": "0.1.1",
    "description": "Formik Developer Tools for debugging React form components",
    "private": true,
    "author": {
        "email": "vitaliy.ptt@gmail.com",
        "url": "https://github.com/petrenkoVitaliy",
        "name": "Vitaliy Petrenko"
    },
    "license": "MIT",
    "dependencies": {
        "classnames": "^2.2.6",
        "node-sass": "^4.14.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.3",
        "yup": "^0.29.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "lint": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
        "prettier": "prettier --write --print-width 120  --trailing-comma all \"**/*.+(ts|tsx|js|jsx)\""
    },
    "pre-commit": [
        "prettier",
        "lint"
    ],
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/node": "^12.12.62",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "@types/yup": "^0.29.8",
        "@types/chrome": "0.0.125",
        "@types/classnames": "^2.2.10",
        "@typescript-eslint/eslint-plugin": "4.0.0",
        "@typescript-eslint/parser": "4.0.0",
        "pre-commit": "^1.2.2",
        "prettier": "^2.1.2",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-prettier": "^3.1.4",
        "typescript": "^3.7.5"
    }
}
